{
	"manifest_version": 2,
	"name": "Google-Calendar-Copy",
	"description": "Chrome extension for quickly copy a item on Google calendar",
	"icons": {
		"16": "icon_16.png",
		"128": "icon_128.png"
	},
	"version": "1.0",
	"content_scripts": [
		{
			"matches": [
				"https://www.google.com/calendar/*",
				"https://google.com/calendar/*",
				"http://www.google.com/calendar/*",
				"http://google.com/calendar/*"
			],
			"js": [
				"content.js"
			]
		}
	]
}
